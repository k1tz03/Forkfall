{
  "expo": {
    "name": "FORKFALL",
    "slug": "forkfall",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#1a1a2e",
    "splash": {
      "backgroundColor": "#1a1a2e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.forkfall.app"
    },
    "android": {
      "package": "com.forkfall.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
